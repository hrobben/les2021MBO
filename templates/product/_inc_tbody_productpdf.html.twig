<tbody>
{% for product in products %}
    <tr>
        <td>{{ product.id }}</td>
        <td>{{ product.code }}</td>
        <td>{{ product.naam }}</td>
        <td>{% if product.category.description is defined %}{{ product.category.description }}{% endif %}</td>
        <td>{{ product.prijs }}</td>
    </tr>
{% else %}
    <tr>
        <td colspan="5">no records found</td>
    </tr>
{% endfor %}
</tbody>
'GET', 'POST'